#include <stdio.h>
#include <math.h>
#include <windows.h>

int main()
{
	//Declarando variables
	int cont = 0;
	int cont2 = 0;


	printf ("\tWHILE\n\n");
	while(cont<10){
		printf ("%d.- Te amo <3\n",cont);
		cont++;
	}
	
	printf ("\t DO - WHILE\n\n");	
	do {
		printf ("%d.- Te odio >_<\n",cont2);
		cont2++;
	} while (cont2 <0):
}
